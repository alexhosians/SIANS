@cython.boundscheck(False)
@cython.wraparound(False)
cdef int Tsitouras54(double[:,:] A, double[:] C, double[:] B, double[:] E):
	"""
	A 7-stage Runge-Kutta 5(4) method due to Tsitouras.
	This method is claimed to be slightly more efficient than the Dormand-Prince 5(4) method.
	
	Coefficients taken from:
	https://doi.org/10.1016/j.camwa.2011.06.002.
	"""
	ArrayChecks(A, C, B, E, 7)
	# Nodes
	C[0] = 0.161
	C[1] = 0.327
	C[2] = 0.9
	C[3] = 0.9800255409045097
	C[4] = 1.0
	C[5] = 1.0
	# Higher order weights
	B[0] =  0.09646076681806523
	B[1] =  0.01
	B[2] =  0.4798896504144996
	B[3] =  1.379008574103742
	B[4] = -3.290069515436081
	B[5] =  2.324710524099774
	B[6] =  0.0
	# Lower order weights
	E[0] =  0.001780011052226
	E[1] =  0.000816434459657
	E[2] = -0.007880878010262
	E[3] =  0.144711007173263
	E[4] = -0.582357165452555
	E[5] =  0.458082105929187
	E[6] =  1.0/66
	# Coupling coefficients
	A[1,1] =  0.3354806554923570

	A[2,1] = -6.359448489975075
	A[2,2] =  4.362295432869581

	A[3,1] = -11.74888356406283
	A[3,2] =  7.495539342889836
	A[3,3] = -0.09249506636175525

	A[4,1] = -12.92096931784711
	A[4,2] =  8.159367898576159
	A[4,3] = -0.07158497328140100
	A[4,4] = -0.02826905039406838

	A[5,0] =  B[0]
	A[5,1] =  B[1]
	A[5,2] =  B[2]
	A[5,3] =  B[3]
	A[5,4] =  B[4]
	A[5,5] =  B[5]
	"""
	cdef Py_ssize_t i, j
	cdef double Asum
	for i in range(5):
		Asum = 0.0
		for j in range(i):
			Asum += A[i,j]
		A[i,0] = C[i] - Asum
	"""
	A[0,0] = C[0]
	A[1,0] = C[1] - A[1,1]
	A[2,0] = C[2] - A[2,1] - A[2,2]
	A[3,0] = C[3] - A[3,1] - A[3,2] - A[3,3]
	A[4,0] = C[4] - A[4,1] - A[4,2] - A[4,3] - A[4,4]


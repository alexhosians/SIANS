cdef int RungeKutta4(double[:,:] A, double[:] C, double[:] B, double[:] E):
	"""
	Classical fourth order Runge Kutta method.
	Adaptive time steppers automatically turned off for this method.
	"""
	ArrayChecks(A, C, B, E, 4)
	# Nodes
	C[0] = 0.5
	C[1] = 0.5
	C[2] = 1.0
	# Coupling coefficients
	A[0,0] =  0.5

	A[1,0] =  0.0
	A[1,1] =  0.5

	A[2,0] =  0.0
	A[2,1] =  0.0
	A[2,2] =  1.0

	# Higher order weights
	B[0] =  1.0/6
	B[1] =  1.0/3
	B[2] =  1.0/3
	B[3] =  1.0/6
		
